<!doctype html>
<html lang="en">
  <head>
    <%- include ('global/partials/header');  %>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <title>Crime and Punishment Datatable</title>
    <meta name="author" content="You">
    <meta name="description" content="Test">
    <meta content="text/html" http-equiv="Content-type" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />

    <!-- Load plotly.js into the DOM -->
  	<script src='https://cdn.plot.ly/plotly-2.6.3.min.js'></script>
  	<script src='https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.17/d3.min.js'></script>
    
  </head>
  <body>
    <%- include ('global/partials/navbar');  %>
    <main role="main" class="container">
      <h1>Test</h1>
      <div class="starter-template">
        <br>
        <h1>Tracking Crime and Punishment</h1>
        <p>This is a cool project.</p>
      </div>
      <div id='mapDiv' style="width:1000px;height:600px;"><!-- Plotly chart will be drawn inside this DIV --></div>
    </main>
    <br>
  <section id="charcter-table">
    <table>
      <tr>
        <th>Jurisdiction</th>
        <th>Year</th>
        <th>Includes_jails</th>
        <th>Prisoner Count</th>
        <th>Crime Reporting Change</th>
        <th>Crimes Estimated</th>
        <th>State Population</th>
        <th>Violent Crime Total</th>
        <th>Murder Manslaughter</th>
        <th>Rape Revised</th>
        <th>Aggravated Assault</th>
        <th>Property Crime Total</th>
        <th>Burglary</th>
        <th>Larceny</th>
        <th>Vehicle Theft</th>
      </tr>
      <% for(let i=1; i<data.length; i++){ %>
      <tr>
        <td><%= data[i].jurisdiction %></td>
        <td><%= data[i].includes_jails %></td>
        <td><%= data[i].year %></td>
        <td><%= data[i].prisoner_count %></td>
        <td><%= data[i].crime_reporting_change %></td>
        <td><%= data[i].crimes_estimated %></td>
        <td><%= data[i].state_population %></td>
        <td><%= data[i].violent_crime_total %></td>
        <td><%= data[i].murder_manslaughter %></td>
        <td><%= data[i].rape_revised %></td>
        <td><%= data[i].robbery %></td>
        <td><%= data[i].agg_assault %></td>
        <td><%= data[i].property_crime_total %></td>
        <td><%= data[i].burglary %></td>
        <td><%= data[i].larceny %></td>
        <td><%= data[i].vehicle_theft %></td>
      </tr>
      <%}%>
    </table>
  </section>
  <%- include ('global/partials/footer');  %>
</body>
<script src='mapGenerator.js'></script>
</html>
